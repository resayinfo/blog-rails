    <div class="footer">
      <a href="tel:732-513-6080">
        <%= image_tag 'phone.png' %>
      </a>
      <a href="mailto:ajmaseda@gmail.com">
        <%= image_tag 'email.png' %>
      </a>
      <a href="https://www.facebook.com/anthony.maseda" target = "blank">
        <%= image_tag 'facebook.png' %>
      </a>
      <a href="http://instagram.com/tony_maseda/" target = "blank">
        <%= image_tag 'instagram.png' %>
      </a>
      <a href="https://www.youtube.com/channel/UCxR1elfy91jLjkdI5FbG2Fg" target = "blank">
        <%= image_tag 'youtube.png' %>
      </a>
      <a href="https://twitter.com/AnthonyMaseda" target = "blank">
        <%= image_tag 'twitter.png' %>
      </a>
      <a href="https://www.linkedin.com/profile/view?id=164611762&trk=nav_responsive_tab_profile" target = "blank">
        <%= image_tag 'linkedin.png' %>
