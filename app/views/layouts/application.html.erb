            <li class="<%= controller.class == WelcomeController  || controller.class == WelcomeController || controller.class == WelcomeController ? 'active' : '' %>"><%= link_to 'Home', root_path, class: 'navbar-brand' %></li>
            <li class="<%= controller.class == ArticlesController  || controller.class == ArticlesController || controller.class == ArticlesController ? 'active' : '' %>"><%= link_to 'Articles', articles_path, class: 'navbar-brand' %></li>
            <li class="<%= controller.class == UsersController  || controller.class == UsersController || controller.class == UsersController ? 'active' : '' %>"><%= link_to 'Users', users_path, class: 'navbar-brand' %></li>
